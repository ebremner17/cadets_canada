{
    "name": "cadets_canada/cadets_canada",
    "description": "A drupal profile for Cadets Canada",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "ebremner17/cadets_canada_theme": {
            "type": "vcs",
            "url": "https://github.com/ebremner17/cadets_canada_theme.git"
        },
        "ebremner17/cc_site_controller": {
            "type": "vcs",
            "url": "https://github.com/ebremner17/cc_site_controller.git"
        },
    },
    "conflict": {
        "drupal/drupal": "*",
        "drupal-custom-module/drupal-custom-module": "*",
        "drupal-custom-theme/drupal-custom-theme": "*"
    },
    "config": {
        "secure-http": false,
        "sort-packages": true,
        "vendor-dir": "../../vendor"
    },
    "minimum-stability": "dev",
    "require": {
        "composer/installers": "^1.7",
        "oomphinc/composer-installers-extender": "v1.1.2",
        "drupal/core": "8.7.7",
        "drupal/layout_builder_modal": "1.0-alpha1",
        "drupal/layout_builder_restrictions": "2.2",
        "ebremner17/cc_site_controller": "dev-8.x-1.x"
    },
    "extra": {
        "installer-types": [
            "drupal",
            "drupal-custom-module"
        ],
        "installer-paths": {
            "../../core": [
                "type:drupal-core"
            ],
            "modules/features/{$name}": [
                "type:drupal-custom-module"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "themes/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
